{
  "name": "coachsearching-web",
  "version": "2.0.0",
  "type": "module",
  "description": "Professional coaching marketplace platform - Find your perfect coach",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint js --ext .js --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint js --ext .js --fix",
    "format": "prettier --write \"js/**/*.js\" \"css/**/*.css\" \"*.html\" \"*.json\"",
    "format:check": "prettier --check \"js/**/*.js\" \"css/**/*.css\" \"*.html\" \"*.json\"",
    "typecheck": "tsc --noEmit --allowJs --checkJs",
    "test": "echo 'Tests run in browser - open test.html'",
    "serve": "npx serve .",
    "validate": "npm run lint && npm run typecheck && npm run format:check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "serve": "^14.2.3",
    "vite": "^5.4.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
